datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

model GitHubPageViews {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  username  String
}

model BlogPageViews {
  id         Int      @id @default(autoincrement())
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  slug       String   @unique
  totalCount Int
}
